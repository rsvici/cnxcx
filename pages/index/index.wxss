/**index.wxss**/
.choice {
  font-size: 22rpx;
  /* margin-top: 80rpx; */
  background-color: #f5f5f5;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  overflow: hidden;
  z-index: 10;
  box-sizing: border-box;
  height: 75rpx;
}

.choice .choiceTop .busi {
  position: absolute;
  left: 40rpx;
  top: 15rpx;
}

.choice .choiceTop .map {
  position: absolute;
  right: 40rpx;
  top: 10rpx;
}

.choice .choiceTop .busi image,
.choice .choiceTop .map image {
  width: 50rpx;
}

.choice .choiceTop {
  height: 60rpx;
  width: 750rpx;
  margin: 0 auto;
  padding-top: 10rpx;
}

.choice .choiceTop form {
  width: 50%;
  background-color: #fff;
  border-radius: 5px;
  height: 55rpx;
  text-align: center;
  padding-left: 60rpx;
  margin: 0 auto;
  position: relative;
  display: block;
}

.choice .choiceTop form image {
  display: block;
  width: 32rpx;
  position: absolute;
  top: 13rpx;
  left: 70rpx;
}

.choice .choiceTop form input {
  display: block;
  margin-left: 75rpx;
  height: 55rpx;
  padding: 0;
  line-height: 55rpx;
  caret-color: rgb(170, 20, 20);
  font-family: PingFangSC-Regular;
  color: #666;
  text-align: left;
}

.choice .choiceTop form input::-webkit-input-placeholder {
  font-family: PingFangSC-Regular;
  line-height: 50rpx;
}

/* 乐活 */
.lehuo {
  position: absolute;
  top: 75rpx;
  width: 750rpx;
  background-color: #f1f1f1;
  padding-bottom: 1vw;
}

.lehuo swiper {
  width: 100%;
  /* height: 260.42rpx; */
}

.lehuo swiper swiper-item {
  width: 100%;
  text-align: right;
  overflow: hidden;
}

.lehuo swiper swiper-item image {
  width: 100%;
  border-radius: 10rpx;
}

/* 按钮 */
/* 按钮 */

.lehuo .lh_nav {
  width: 100vw;
  /* height: 38vw; */
  background-color: #fff;
  overflow: hidden;
  padding-bottom: 3vw;
}

.lehuo .lh_nav>view {
  width: 20%;
  float: left;
  height: 18vw;
  padding: 4vw 4vw 2.5vw;
  box-sizing: border-box;
}

.lehuo .lh_nav view image {
  width: 100%;
}
.lehuo .lh_nav .typeName{
  width: 100%;
  text-align: center;
}

/* 精彩 */

.lehuo .lehuo_teach {
  width: 100%;
  height: 376.94rpx;
  margin-top: 4vw;
  background-image: url('http://www.appsun.com.cn/www/fy/changning/image/lehuo/teachbg.jpg');
  background-size: cover;
  background-position: 50% 50%;
  overflow: hidden;
  position: relative;
}

.lehuo .lehuo_teach .teach_banner {
  margin-top: 10.5vw;
  width: 100%;
  min-height: 300rpx;
}

.lehuo .lehuo_teach swiper-item image {
  width: 515rpx;
  height: 270rpx;
}

.lehuo .more {
  position: absolute;
  right: 5vw;
  top: 13px;
  width: 10vw;
}

.lehuo .more image {
  width: 100%;
}

/* 热门 */
.lehuo .lehuo_space {
  width: 100%;
  height: 60.74vw;
  margin-top: 4vw;
  background-image: url('http://www.appsun.com.cn/www/fy/changning/image/lehuo/spacebg.jpg');
  background-size: cover;
  background-position: 50% 50%;
  overflow: hidden;
  position: relative;
}

.lehuo .lehuo_space swiper.space_banner {
  margin-top: 12.5vw;
  width: 100%;

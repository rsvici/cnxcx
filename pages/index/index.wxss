.lehuo .lehuo_space .more {
  top: 20px;
}

/* 运动 */
.lehuo .lehuo_sport {
  width: 100%;
  height: 36.574vw;
  margin-top: 4vw;
  background-image: url('http://www.appsun.com.cn/www/fy/changning/image/lehuo/sport.jpg');
  background-size: cover;
  background-position: 50% 50%;
  overflow: hidden;
  position: relative;
}

.lehuo .lehuo_sport swiper.sport_banner {
  margin-top: 10vw;
  width: 100%;
  height: 26.574vw;
}

.lehuo .lehuo_sport swiper-item {
  padding: 10rpx;
  box-sizing: border-box;
}

.lehuo .lehuo_sport swiper-item image {
  width: 280rpx;
  height: 165rpx;
}
.lehuo .lehuo_sport .sportText{
  padding: 5rpx 0;
  position: absolute;
  left: 15%;
  bottom: 5rpx;
  width: 70%;
  text-align: center;
  font-size: 20rpx;
  line-height: 22rpx;
  background-color: #fff;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}



/* 列表 */
/* 列表 */
.choicelistAll {
  position: relative;
  /* margin-top: 4vw; */
}

.choicelist .title {
  width: 100%;
  height: 12vw;
  background-image: url('http://www.appsun.com.cn/www/fy/changning/imgs/activelist/list.png');
  background-size: contain;
  background-repeat: no-repeat;
  z-index: 10;
  background-color: #fff;
  margin-top: 3rpx;
}

.choicelist .title .li {
  font-size: 24rpx;
  width: 33%;
  display: inline-block;
  text-align: center;
  line-height: 6vw;
  color: #6A6A6A;
}

.choicelist .title .li image {
  width: 10%;
  display: inline-block;
  margin-left: 3%;
}

.fixtype {
  position: absolute;
  top: 60rpx;
  font-size: 20rpx;
  width: 100%;
  padding: 0 20rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #B1B1B1;
  background-color: #fff;
  z-index: 10;
}

.fixtype .f_li {
  float: left;
  width: 20%;
  text-align: center;
  margin-top: 20rpx;
}

.fixtype .f_li text {
  display: block;
  width: 76%;
  border: 1px solid #DBEFFF;
  border-radius: 10rpx;
  line-height: 30rpx;
}

/* 活动列表 */
.Alist {
  position: absolute;
  /* top: 60rpx; */
  top: 37rpx;
  width: 100%;
  background-color: #fff;
}


.Alist .Aitem {
  min-height: 100rpx;
  width: 94%;
  margin: 0 auto;
  padding: 10rpx 0;
  box-sizing: border-box;
  overflow: hidden;
  border-bottom: 1px solid #E6E6E6;
}

.Alist .Aitem .left {
  float: left;
  width: 25%;
}

.Alist .Aitem .left image {
  width: 100%;
  border-radius: 10rpx;
}

.Alist .Aitem .right {
  margin-left: 3%;
  padding-top: 20rpx;
  float: left;
  width: 70%;
}

.itemword text,
.itemTitle text {
  line-height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.itemword2 text {
  line-height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.Alist .right .itemTitle text {
  font-size: 30rpx;
  line-height: 40rpx;
  padding-left: 10rpx;
  font-weight: 700;
}

.Alist .Aitem .right .Rleft {
  width: 20%;
  float: left;
  padding-top: 50rpx;
}

.Alist .Aitem .right .Rleft view {
  height: 50rpx;
  font-size: 22rpx;
  text-align: right;
  font-weight: 700;
}

.Alist .Aitem .right .Rright {
  width: 75%;
  float: left;
  padding-top: 50rpx;
  margin-left: 3%;
}

.Alist .Aitem .right .Rright view {
  width: 100%;
  overflow: hidden;
  height: 50rpx;
}

.Alist .Aitem .right .Rright view text {
  font-size: 21rpx;
  color: #444;
}

.Alist .Aitem .right .Rright .itemword2 {
  height: 60rpx;
}


.lehuo .home image {
  /* width: 695rpx; */
}

.home .swiper-item {
  border-radius: 6px;
  width: 695rpx;
  overflow: hidden;
}

.home .le-img {
  width: 100%;
  height:97%;
  text-align: center;
  display: block;
  transform: scale(0.8);
  transition: all 0.3s ease;
  overflow: hidden;
  border-radius: 6px;
}

.home .le-img.le-active {
  transform: scale(1);
}
.home .le-img image{
  width: 100%;
  height:100%;
  border-radius: 6px;
  display: block;
  margin: auto;
}

